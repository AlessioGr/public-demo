{
  "name": "payload-public-demo",
  "description": "Payload Demo",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "pre:debug": "pnpm copyfiles && pnpm build:server",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payloadold": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload-cjs build --cjs",
    "build:server": "tsc",
    "build": "pnpm generate:types && pnpm copyfiles && pnpm build:payload && pnpm build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload-cjs generate:types -cjs",
    "generate:graphql-schema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload-cjs generate:graphqlSchema -cjs"
  },
  "dependencies": {
    "@faker-js/faker": "^6.3.1",
    "@payloadcms/db-mongodb": "file:/Users/alessio/Documents/GitHub/payload/packages/db-mongodb/",
    "@yarnpkg/lockfile": "^1.1.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^4.3.0",
    "mongodb-memory-server": "^8.15.1",
    "node-cron": "^3.0.0",
    "payload": "file:/Users/alessio/Documents/GitHub/payload/packages/payload/",
    "qs": "^6.10.3",
    "slate": "^0.94.1",
    "slate-react": "^0.98.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node-cron": "^3.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  }
}
